{
    "name": "modern-js-tree",
    "version": "1.0.5",
    "description": "Explore JS state stores in a navigable tree format",
    "license": "MIT",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "authors": [
        "Danilo Alonso <damusix@gmail.com>"
    ],
    "scripts": {
        "start": "parcel example/index.html",
        "build": "rollup -c"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/damusix/modern-js-tree.git"
    },
    "bugs": {
        "url": "https://github.com/damusix/modern-js-tree/issues"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "keywords": [
        "json",
        "viewer",
        "devtools",
        "explorer",
        "tree"
    ],
    "devDependencies": {
        "@riot-tools/state-utils": "^1.1.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@types/node": "^14.14.31",
        "cssnano": "^5.0.2",
        "postcss": "^8.2.14",
        "rollup": "^2.47.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-generate-package-json": "^3.2.0",
        "rollup-plugin-scss": "^2.6.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "sass": "^1.32.8",
        "typescript": "^4.2.2"
    },
    "release": {
        "ci": false,
        "branches": [
            "main",
            {
                "name": "next",
                "prerelease": true
            }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/changelog",
            "@semantic-release/release-notes-generator",
            "@semantic-release/npm",
            "@semantic-release/git"
        ]
    }
}
